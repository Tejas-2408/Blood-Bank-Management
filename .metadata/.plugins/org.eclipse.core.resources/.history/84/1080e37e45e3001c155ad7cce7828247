<%
int id=1;
try{
	Connection con = ConnectionProvider.getCon();
	Statement st = con.createStatement();
	ResultSet rs = st.executeQuery("select max(id) from donor");
	if(rs.next()){
		id = rs.getInt(1);
		id++;
		
	}
}
catch(Exception e){
	
}

